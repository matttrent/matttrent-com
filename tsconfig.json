{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@css/*": [
        "src/css/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@mttypes/*": [
        "src/types/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}
